# Core Configuration
TIMEZONE=America/New_York
DEBUG_MODE=false
TEST_MODE=false
FORCE_LIVE_API=false

# Storage (DigitalOcean Spaces or S3)
SPACES_BASE_PREFIX=data
SPACES_ACCESS_KEY_ID=your_access_key_here
SPACES_SECRET_ACCESS_KEY=your_secret_key_here
SPACES_BUCKET_NAME=your-bucket-name
SPACES_REGION=nyc3

# Data Retention
INTRADAY_TRIM_DAYS=7
INTRADAY_EXCLUDE_TODAY=false
PROCESS_MANUAL_TICKERS=true

# API (Alpha Vantage)
ALPHA_VANTAGE_API_KEY=your_api_key_here
API_RATE_LIMIT_PER_MIN=5

# Orchestration
HEALTHCHECK_DEGRADED_AFTER_MINS=3
HEALTHCHECK_FAIL_AFTER_MINS=10